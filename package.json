{
	"name": "app",
	"version": "0.0.1",
	"private": true,
	"prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"migrate:dev": "npx prisma migrate dev --preview-feature",
		"migrate:dev:create": "npx prisma migrate dev --create-only --preview-feature",
		"migrate:reset": "npx prisma migrate reset --preview-feature",
		"migrate:deploy": "npx prisma migrate deploy --preview-feature",
		"migrate:status": "npx prisma migrate status --preview-feature",
		"migrate:resolve": "npx prisma migrate resolve --preview-feature",
		"prisma:studio": "npx prisma studio",
		"zen:generate": "npx zenstack generate",
		"zen:push": "npx prisma db push",
		"zen:repl": "npx zenstack repl",
		"start:db": "npm run zen:generate && npm run migrate:dev && npm run seed",
		"seed": "npx prisma db seed",
		"postinstall": "npm run zen:generate",
		"docker:db:up": "docker-compose -f docker-compose.db.yml up -d",
		"docker:db:down": "docker-compose -f docker-compose.db.yml down"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/eslint": "8.56.0",
		"@types/jsonwebtoken": "^9.0.5",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@zenstackhq/trpc": "^1.6.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.7.1",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"zenstack": "1.6.1"
	},
	"type": "module",
	"dependencies": {
		"@auth/prisma-adapter": "^1.0.12",
		"@auth/sveltekit": "^0.5.0",
		"@prisma/client": "^5.7.1",
		"@trpc/client": "^10.45.0",
		"@trpc/server": "^10.45.0",
		"@zenstackhq/runtime": "1.6.1",
		"@zenstackhq/server": "^1.6.1",
		"bcryptjs": "^2.4.3",
		"jsonwebtoken": "^9.0.2",
		"trpc-sveltekit": "^3.5.22",
		"zod": "^3.22.4"
	}
}
